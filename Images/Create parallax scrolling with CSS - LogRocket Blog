  .mktoForm, .mktoForm * { box-sizing: border-box; padding: 10px; }
  .mktoForm .mktoGutter, .mktoForm .mktoOffset { display: none; }
  .mktoForm .mktoFormCol .mktoLabel { text-align: left; width: 100%; }
